package com.sub.blc;

public class EmplyeeProperties {
    int employeeId;
    String employeeName;
    Double employeeSalary;
    public void setProductData(int id, String name, double price) 
	{
    	employeeId=id;
    	employeeName=name;
    	employeeSalary=price;
     }
     public void getProductInfo()
     {
    	 System.out.println("Product Id is:"+employeeId);
    	 System.out.println("Product Name is:"+employeeName);
    	 System.out.println("Product Price is:"+employeeSalary);
    	 
     }

}
