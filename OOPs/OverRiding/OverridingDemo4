package com.sub.overriding;
class SuperClass
{
	public void show()
	{
		IO.println("Show method of super class");
	}
}
class SubClass extends SuperClass
{
	public void show()
	{
		IO.println("Show method of sub class");
	}
	
}
public class OverridingDemo4 {
	public static void main(String[] args) {
		SuperClass superClass=new SubClass();
		superClass.show();

	}

}
